{
  "appId": "com.bottledocean.app",
  "appName": "Bottled Ocean",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "ios": {
    "contentInset": "never",
    "allowsLinkPreview": false
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "StatusBar": {
      "style": "DARK",
      "overlaysWebView": false,
      "ios": {
        "style": "Dark"
      }
    },
    "Geolocation": {
      "permissions": {
        "android": {
          "location": "always"
        }
      }
    }
  }
}
